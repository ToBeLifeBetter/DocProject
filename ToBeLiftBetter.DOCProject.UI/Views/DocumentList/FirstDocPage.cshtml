
@{
    ViewBag.Title = "DocPage";
}

@section css{
    .layui-tree-entry{
    height:30px;
    }
}

<!--中间文章正文-->
<div class="col-sm-6 this-content" id="content">

    <div style="height: 80px;" id="section" class="active"></div>

    <!-- start 可替换正文部分-->
    <h2>@ViewData.Model </h2>

    <!-- end 可替换正文部分-->
</div>
<!--右边导航-->
<div class="col-sm-3 this-right-nav" id="rightbox">
    <!--start 可替换右边导航-->
    <div class="col-xs-3" id="myScrollspy">
        <ul class="nav nav-tabs nav-stacked rightnav" data-spy="affix">
            @{
                bool flag = true;
                foreach (var item in (Dictionary<string, string>)ViewBag.list)
                {
                    if (flag)
                    {
                        <li class="active"><a href="#@item.Key" id="frist">@item.Value</a></li>
                        flag = false;
                    }
                    <li><a href="#@item.Key" id="frist">@item.Value</a></li>
                }
            }

        </ul>
    </div>
    <!--end 可替换右边导航-->
</div>